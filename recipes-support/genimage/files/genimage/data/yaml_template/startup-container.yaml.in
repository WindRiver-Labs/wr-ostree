# This Yaml file is generated by the creation of @IMAGE_NAME@.docker-image.tar.bz2
# which is used by genimage to load and run @IMAGE_NAME@.docker-image.tar.bz2
image_type:
- ustart
packages:
- startup-container
- docker
include-container-images:
- container-name: @IMAGE_NAME@
  copy_to_target: copy deploy/@IMAGE_NAME@.docker-image.tar.bz2 $IMAGE_ROOTFS/opt/docker-images/
  add_to_docker: docker load -i /opt/docker-images/@IMAGE_NAME@.docker-image.tar.bz2
  docker-run: docker run -itd @IMAGE_NAME@ /bin/sh
