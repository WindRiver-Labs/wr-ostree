# This Yaml file is generated by the creation of @IMAGE_NAME@.docker-image.tar.bz2
# which is used by genimage to load and run @IMAGE_NAME@.docker-image.tar.bz2
image_type:
- ustart
packages:
- startup-container
- docker
include-container-images:
- container-name: @IMAGE_NAME@
  copy-container: cp deploy/@IMAGE_NAME@.docker-image.tar.bz2 $IMAGE_ROOTFS/var/docker-images/
  add-container: docker load -i /var/docker-images/@IMAGE_NAME@.docker-image.tar.bz2
  run-container: docker run -itd @IMAGE_NAME@ /bin/sh
